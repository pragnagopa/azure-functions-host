{
  "entryPoint": "queueTriggerHandler",
  "bindings": [
    {
      "name": "myQueueItem",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "test-input-node",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "$return",
      "type": "queue",
      "direction": "out",
      "queueName": "myqueue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}